[
    {
        "title": "GROUP_TITLE_SEASONEPISODE",
        "regex": "^(?:\\[[\\w ]+\\])? ([\\w,. \\!-]+) - (S\\d\\dE\\d\\d)",
        "groups": "title,seasonepisode"
    },
    {
        "title": "GROUP_TITLE_EPISODE",
        "regex": "^(?:\\[[\\w ]+\\])? ([\\w -!]+) (- \\d\\d)",
        "groups": "title,episode",
        "replacers": [{
            "pattern": "{episode}",
            "remover": "- ",
            "replace": "- S{season}E{episode}"
            }
        ]
    },
    {
        "title": "GROUP_TITLE_SEASONEPISODE_NODASH",
        "regex": "^(?:\\[[\\w ]+\\])? ([\\w,. \\!-]+) (S\\d\\dE\\d\\d)",
        "groups": "title,seasonepisode"        
    },
    {
        "title": "GROUP_TITLE_SEASONEPISODE_NODASH",
        "regex": "^(?:\\[[\\w ]+\\])? ([\\w,. !-]+) S(\\d)E(\\d\\d)",
        "groups": "title,season,episode"
    },
    {
        "title": "GROUP_TITLE_OVA",
        "regex": "^(?:\\[[\\w ]+\\])? ([\\w,. !-]+) (OVA)",
        "groups": "title,episode"
    },
    {
        "title": "DOTTITLE_SEASONEPISODE",
        "regex": "^([\\w\\-.]+)[\\.]*(S\\d\\dE\\d\\d)",
        "groups": "title,seasonepisode",
        "replacers": [{
            "pattern": ".",
            "remover": "",
            "replace": " "
        }]
    },
    {
        "title": "Erai-raws",
        "regex": "^\\[Erai-raws\\] ([\\w\\d\\(\\) ]+) - (\\d\\d)",
        "groups": "title,episode"
    }
]